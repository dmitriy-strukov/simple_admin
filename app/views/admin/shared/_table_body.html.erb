<tbody class="table__body">
    <% resources.each do |resource| %>
      <tr class="table__row">
        <% resource_fields.each do |field| %>
          <%= render "#{field.entity_field_type.template}/#{field.display}", field: field, resource: resource %>
        <% end %>

        <%= render 'admin/shared/collection_actions', resource: resource %>
      </tr>
    <% end %>
</tbody>
