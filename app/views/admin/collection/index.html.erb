<section class="catalog-page">
  <header class="item-page__header">
    <h3 class="item-page__title"><%= t("admin.#{@collection_name}.index.title") %></h3>
    <%= link_to t('admin.create'), resource_link(@model_instance, :new, nil, :admin), class: 'button item-page__submit' %>
  </header>

  <table class="table">
    <%= render 'admin/shared/table_header', field_names: @resource_fields.pluck(:name) %>
    <%= render 'admin/shared/table_body', resources: @resources, resource_fields: @resource_fields, namespace: :admin %>
  </table>
</section>
